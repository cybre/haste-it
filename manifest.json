{
    "background": {
        "persistent": false,
        "scripts": [ "jquery.min.js", "background.js" ]
    },
    "description": "Posts selected text to Hastebin.",
   "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },
    "manifest_version": 2,
    "name": "Haste it!",
    "permissions": [
        "http://*/*",
        "https://*/*",
        "contextMenus",
        "tabs"
    ],
    "version": "1.0"
}

